{
  "name": "strant",
  "version": "0.0.1",
  "description": "A huge catalog of dynamic Stencil web components to reuse for ease of web development",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/strant/strant.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TommyRabes/strant.git"
  },
  "keywords": [
    "stencil",
    "web-components",
    "design-system",
    "strant"
  ],
  "author": "Tommy",
  "bugs": {
    "url": "https://github.com/TommyRabes/strant/issues"
  },
  "homepage": "https://github.com/TommyRabes/strant#readme",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "2.22.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0"
  },
  "license": "MIT"
}
